{
  "name": "@acme/db",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "default": "./dist/client.js"
    },
    "./schema": {
      "types": "./dist/schema.d.ts",
      "default": "./dist/schema.js"
    }
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "dev": "tsc",
    "clean": "rm -rf .turbo dist node_modules",
    "push": "pnpm with-env drizzle-kit push",
    "db:gen": "pnpm with-env drizzle-kit generate",
    "db:migrate": "pnpm with-env drizzle-kit migrate",
    "db:clear": "tsx ./src/db/clear.ts",
    "db:seed": "tsx ./src/db/seed.ts",
    "db:reset": "npm run db:clear && npm run db:migrate && npm run db:seed",
    "db:studio": "pnpm with-env drizzle-kit studio",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@t3-oss/env-core": "^0.10.1",
    "drizzle-orm": "^0.31.4",
    "drizzle-zod": "^0.5.1",
    "postgres": "^3.4.4",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@acme/tsconfig": "workspace:*",
    "tsx": "^4.16.2",
    "dotenv-cli": "^7.4.2",
    "drizzle-kit": "^0.22.8",
    "typescript": "catalog:"
  }
}
